<Window x:Class="POP_RS18_2012GUI.UI.NamestajWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NamestajWindow" Height="150" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="81"/>
            <ColumnDefinition Width="211"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="19*"/>
            <RowDefinition Height="21*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.Column="0" Content="Naziv:" FontSize="15"></Label>
        <TextBox Grid.Row="0" Grid.Column="1" Width="Auto" Name="tbNaziv"/>
        <Button Grid.Row="1" Content="Sacuvaj" Grid.Column="1" Margin="130,0,0,0" />
        <Button Grid.Row="1" Content="Izadji" Click="Izadji"/>
    </Grid>
</Window>
